{
  "name": "calculateur-oakaddins",
  "version": "1.0.0",
  "description": "dev",
  "main": "host.js",
  "scripts": {
    "build": "webpack --progress --mode=production",
    "dev-nohost": "webpack --watch --progress --mode=development",
    "dev": "concurrently -k -n \"Webpack,Host\" -p \"[{name}]\" -c \"blue,green\" \"webpack --watch --progress --mode=development\" \"node host.js --path=build --port=6600\"",
    "doc": "typedoc src --out ./docs/typeDoc",
    "doc-github": "typedoc src --out ./docs/typeDoc-github --theme ./node_modules/typedoc-github-wiki-theme/dist",
    "docUnsatable": "jsdoc -c docs/jsDocConfig.json -p",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/P0ulpy/Configurateur-OakAddins.git"
  },
  "author": "P0ulpy",
