  "bugs": {
    "url": "https://github.com/P0ulpy/Configurateur-OakAddins/issues"
  },
  "homepage": "https://github.com/P0ulpy/Configurateur-OakAddins#readme",
  "dependencies": {
    "copy-webpack-plugin": "^6.4.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "jsonc": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "concurrently": "^6.1.0",
    "express": "^4.17.1",
    "minimist": "^1.2.5",
    "typedoc": "^0.21.0",
    "typedoc-github-wiki-theme": "^0.4.2",
    "typedoc-plugin-markdown": "^3.10.0"
  }
}
